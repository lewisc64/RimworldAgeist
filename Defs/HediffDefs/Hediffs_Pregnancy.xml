<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <HediffDef>
    <defName>HumanPregnancy</defName>
    <hediffClass>Ageist.Hediff_HumanPregnancy</hediffClass>
    <label>human pregnant</label>
    <defaultLabelColor>(0.7, 1.0, 0.7)</defaultLabelColor>
    <isBad>false</isBad>
    <initialSeverity>0.1</initialSeverity>
    <labelNoun>pregnancy</labelNoun>
    <stages>
      <li>
        <label>early-stage</label>
        <minSeverity>0.1</minSeverity>
      </li>
      <li>
        <label>middle-stage</label>
        <minSeverity>0.2</minSeverity>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.2</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>late-stage</label>
        <minSeverity>0.3</minSeverity>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.4</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>overdue</label>
        <minSeverity>0.4</minSeverity>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.4</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>water broken</label>
        <painOffset>0.2</painOffset>
        <minSeverity>0.5</minSeverity>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <setMax>0.3</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <label>labor</label>
        <painOffset>0.5</painOffset>
        <minSeverity>0.6</minSeverity>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <setMax>0.3</setMax>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>